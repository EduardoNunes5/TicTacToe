package game;

public class UI {
	
	
	
	private void printBoard(String[][] board) {
		int size = board.length;
		for(int i = 0; i < size; i++) {
			for(int j = 0; j < size; j++) {
				if(board[i][j] == null || "".equals(board[i][j].trim())) {
					System.out.println("   ");
				}
				else {
					System.out.println(" " + board[i][j] + " ");
				}
			}
			
			System.out.println();
			System.out.println("-------------");
		}
	}

		

}
